{
    "template": "docker-*",
    "mappings": {
        "fluentd": {
            "_all": {"enabled": false},
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "container_id": {
                    "type": "keyword"
                },
                "hostname": {
                    "type": "keyword"
                },
                "hostname_md5": {
                    "type": "keyword"
                },
                "log": {
                    "type": "keyword", "index": false
                },
                "stream": {
                    "type": "keyword"
                },
                "time_nano": {
                    "type": "long", "coerce": true
                }
            }
        }
    }
}
