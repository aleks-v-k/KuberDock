{
    "template": "syslog-*",
    "mappings": {
        "fluentd": {
            "_all": {"enabled": false},
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "host": {
                    "type": "keyword"
                },
                "host_md5": {
                    "type": "keyword"
                },
                "time_nano": {
                    "type": "long", "coerce": true
                },
                "ident":{
                    "type":"keyword"
                },
                "message":{
                    "type": "keyword", "index": false
                }
            }
        }
    }
}

